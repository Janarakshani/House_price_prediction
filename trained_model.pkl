import pandas as pd
from sklearn.linear_model import LinearRegression
import pickle

# Example dataset â€” replace this with your actual data
data = pd.read_csv("housing-3.csv")

# Drop column if needed and prepare data
X = data.drop(['MEDV'], axis=1)  # Features
y = data['MEDV']                 # Target

# Train model
model = LinearRegression()
model.fit(X, y)

# Save the model to a file
with open('trained_model.pkl', 'wb') as f:
    pickle.dump(model, f)

print("Model trained and saved as trained_model.pkl")
